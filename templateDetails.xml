	<config>
	<fields name="params">
		<fieldset name="basic">
			<field
			name="colorVariation"
			type="list"
			default="white"
			label="Color Variation"
			description="Base Color of template">
				<option
				value="blue">blue</option>
				<option
				value="red">red</option>
			</field>
		</fieldset>
	</fields>
	</config>
